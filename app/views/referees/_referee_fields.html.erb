<div class ="form-group">
		<%= f.label :name "Name" %>
		<% f.text_field :name %>
	</div>
	
	<div class="form-group">
		<%= f.label :rules_url "Rules" %>
		<%= f.text_field :rules_url, class: 'form-control' %>
	</div>
	
	<div class="form-group">
		<%= f.label :players_per_game "Players" %>
		<%= f.select :players_per_game, 1..10, {} ,class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :upload "Upload file" %>
		<%= f.text_field :upload %>
	</div>